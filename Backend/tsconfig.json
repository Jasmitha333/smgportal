{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "outDir": "functions/lib",
        "sourceMap": true,
        "strict": true,
        "target": "es2017",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "include": [
        "functions/src/**/*"
    ],
    "exclude": [
        "node_modules",
        "functions/lib"
    ]
}